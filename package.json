{
  "name": "nextjs-with-typescript",
  "version": "5.0.0",
  "description": "Internet Computer nextjs and typescript starter application",
  "keywords": [
    "Internet Computer",
    "Motoko",
    "JavaScript",
    "TypeScript",
    "Canister"
  ],
  "scripts": {
    "dev": "bash dfx.frontend_build.sh && next dev",
    "build": "bash dfx.frontend_build.sh && next build && next export",
    "start": "bash dfx.frontend_build.sh && next start",
    "lint": "next lint --dir src",
    "lint:fix": "yarn lint --fix",
    "export": "bash dfx.frontend_build.sh && next export",
    "post-update": "echo \"codesandbox preview only, need an update\" && yarn upgrade --latest"
  },
  "dependencies": {
    "@dfinity/agent": "^0.15.1",
    "@dfinity/auth-client": "^0.15.1",
    "@dfinity/candid": "^0.15.1",
    "@dfinity/identity": "^0.15.1",
    "@dfinity/principal": "^0.15.1",
    "@emotion/cache": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.5",
    "@internet-identity-labs/react-ic-ii-auth": "^0.0.19",
    "@mui/icons-material": "^5.11.0",
    "@mui/lab": "^5.0.0-alpha.115",
    "@mui/material": "^5.11.3",
    "@nfid/wallet": "^1.1.0",
    "@types/recoil": "^0.0.9",
    "add": "^2.0.6",
    "ictool": "^0.0.7",
    "next": "^12",
    "next-auth": "^4.18.8",
    "next-session": "^4.0.5",
    "react": "^17",
    "react-dom": "^17",
    "react-hook-form": "^7.43.1",
    "react-hot-toast": "^2.4.0",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "eslint": "^8.31.0",
    "eslint-config-next": "^13.1.1",
    "typescript": "^4.9.4"
  }
}
